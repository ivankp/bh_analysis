<!-- Input BlackHat ntuple properties -->
<!--
  alphas="" or omitted: all powers of \alpha_{s} at the same chosen scale
  alphas="two_mH": two powers of \alpha_{s} at mass of Higgs, the rest at a chosen scale
-->
<bh_format alphas="" />

<!-- Definitions of energies of the scales -->
<!--
  Ht = \sum_i^{all} p_{T,i}
    Example: <Ht name="Ht2" frac="0.5" />
  Ht_Higgs = \sqrt{m_H^2 + p_{T,H}^2} + \sum_i^{partons} p_{T,i}
    Example: <Ht_Higgs name="Ht2" frac="0.5" />
  fixed = energy in GeV
    Example: <fixed name="mH" val="125" />
  fac_default = original ntuple factorization scale
    Example: <fac_default name="fac_def" />
  ren_default = original ntuple renormalization scale
    Example: <ren_default name="ren_def" />
-->
<energies>
  <Ht_Higgs name="0.05Ht" frac="0.05" />
  <Ht_Higgs name="0.10Ht" frac="0.10" />
  <Ht_Higgs name="0.15Ht" frac="0.15" />
  <Ht_Higgs name="0.20Ht" frac="0.20" />
  <Ht_Higgs name="0.25Ht" frac="0.25" />
  <Ht_Higgs name="0.30Ht" frac="0.30" />
  <Ht_Higgs name="0.35Ht" frac="0.35" />
  <Ht_Higgs name="0.40Ht" frac="0.40" />
  <Ht_Higgs name="0.45Ht" frac="0.45" />
  <Ht_Higgs name="0.50Ht" frac="0.50" />
  <Ht_Higgs name="0.55Ht" frac="0.55" />
  <Ht_Higgs name="0.60Ht" frac="0.60" />
  <Ht_Higgs name="0.65Ht" frac="0.65" />
  <Ht_Higgs name="0.70Ht" frac="0.70" />
  <Ht_Higgs name="0.75Ht" frac="0.75" />
  <Ht_Higgs name="0.80Ht" frac="0.80" />
  <Ht_Higgs name="0.85Ht" frac="0.85" />
  <Ht_Higgs name="0.90Ht" frac="0.90" />
  <Ht_Higgs name="0.95Ht" frac="0.95" />
  <Ht_Higgs name="1.00Ht" frac="1.00" />
</energies>

