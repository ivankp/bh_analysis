<analysis>
  <particles>
    <!-- name can't be: jet, jets, jet1, jet2, ... -->
    <particle name="H" pid="25" />

    <particle name="W" pt_cut="30">
      <particle name="el" pid="11" />
      <particle name="nu" pid="-12" />
    </particle>
  </particles>

  <jets num="3" pt_cut="30" eta_cut="4.4" />

  <histograms>
    <hist var="pT"   particles="H,H_nj,H_n+j,jets" /> <!-- n = max num of jets -->
    <hist var="eta"  particles="H,H_nj,H_n+j,jets" />
    <hist var="mass" particles="H,jets" />
    <hist var="tau"  particles="H,jets" />
  </histograms>
</analysis>
