<!-- Input BlackHat ntuple properties -->
<!--
  alphas="" or omitted: all powers of \alpha_{s} at the same chosen scale
  alphas="two_mH": two powers of \alpha_{s} at mass of Higgs, the rest at a chosen scale
-->
<bh_format alphas="" />

<!-- Definitions of energies of the scales -->
<!--
  Ht = \sum_i^{all} p_{T,i}
    Example: <Ht name="Ht2" frac="0.5" />
  Ht_Higgs = \sqrt{m_H^2 + p_{T,H}^2} + \sum_i^{partons} p_{T,i}
    Example: <Ht_Higgs name="Ht2" frac="0.5" />
  fixed = energy in GeV
    Example: <fixed name="mH" val="125" />
  fac_default = original ntuple factorization scale
    Example: <fac_default name="fac_def" />
  ren_default = original ntuple renormalization scale
    Example: <ren_default name="ren_def" />
-->
<energies>
  <Ht_Higgs name="0.125Ht" frac="0.125" />
  <Ht_Higgs name="0.25Ht" frac="0.25" />
  <Ht_Higgs name="0.375Ht" frac="0.375" />
  <Ht_Higgs name="0.5Ht" frac="0.5" />
  <Ht_Higgs name="0.625Ht" frac="0.625" />
  <Ht_Higgs name="0.75Ht" frac="0.75" />
  <Ht_Higgs name="0.875Ht" frac="0.875" />
  <Ht_Higgs name="1Ht" frac="1" />
  <Ht_Higgs name="1.125Ht" frac="1.125" />
  <Ht_Higgs name="1.25Ht" frac="1.25" />
  <Ht_Higgs name="1.375Ht" frac="1.375" />
  <Ht_Higgs name="1.5Ht" frac="1.5" />
  <Ht_Higgs name="1.625Ht" frac="1.625" />
  <Ht_Higgs name="1.75Ht" frac="1.75" />
  <Ht_Higgs name="1.875Ht" frac="1.875" />
  <Ht_Higgs name="2Ht" frac="2" />
  <Ht_Higgs name="2.125Ht" frac="2.125" />
  <Ht_Higgs name="2.25Ht" frac="2.25" />
  <Ht_Higgs name="2.375Ht" frac="2.375" />
  <Ht_Higgs name="2.5Ht" frac="2.5" />
  <Ht_Higgs name="2.625Ht" frac="2.625" />
  <Ht_Higgs name="2.75Ht" frac="2.75" />
  <Ht_Higgs name="2.875Ht" frac="2.875" />
  <Ht_Higgs name="3Ht" frac="3" />
  <Ht_Higgs name="3.125Ht" frac="3.125" />
  <Ht_Higgs name="3.25Ht" frac="3.25" />
  <Ht_Higgs name="3.375Ht" frac="3.375" />
  <Ht_Higgs name="3.5Ht" frac="3.5" />
  <Ht_Higgs name="3.625Ht" frac="3.625" />
